<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane fx:id="nutrientInfoIngrContainer" alignment="CENTER" hgap="10.0" maxWidth="300.0" minWidth="200.0" prefWidth="250.0"
          style="-fx-border-style: solid;" vgap="10.0" xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="au.edu.sydney.soft3202.majorproject.controller.NutrientInfoIngredientController">
    <Label fx:id="nutrientInfoIngrText" alignment="CENTER" contentDisplay="CENTER" text="Ingredient Label"
           textAlignment="CENTER" wrapText="true" GridPane.columnSpan="2" GridPane.halignment="CENTER"
           GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
        <font>
            <Font name="System Bold" size="15.0"/>
        </font>
    </Label>
    <Label text="Measure:" wrapText="true" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS"/>
    <Label text="Quantity:" wrapText="true" GridPane.rowIndex="2" GridPane.vgrow="ALWAYS"/>
    <Label fx:id="nutrientInfoMeasureText" text="Measure Value" wrapText="true" GridPane.columnIndex="1"
           GridPane.rowIndex="1" GridPane.vgrow="ALWAYS"/>
    <Label fx:id="nutrientInfoIngrQuantityText" text="Quantity Value" wrapText="true" GridPane.columnIndex="1"
           GridPane.rowIndex="2" GridPane.vgrow="ALWAYS"/>
    <Label text="Retained weight:" wrapText="true" GridPane.rowIndex="3"/>
    <Label fx:id="nutrientInfoIngrRetainedWeightText" text="Retained weight Value" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    <Label text="Weight:" wrapText="true" GridPane.rowIndex="4"/>
    <Label fx:id="nutrientInfoIngrWeightText" text="Weight Value" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS"/>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
